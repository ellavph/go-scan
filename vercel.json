{
	"devCommand": "uvicorn app.main:app --reload",
	"builds": [{ "src": "app/main.py", "use": "@vercel/python" }],
	"routes": [
		{ "src": "/", "dest": "app/main.py" },
		{ "src": "/docs", "dest": "app/main.py" },
		{ "src": "/openapi.json", "dest": "app/main.py" },
    	{ "src": "/user/(.*?)", "dest": "app/main.py" },
    	{ "src": "/order/(.*?)", "dest": "app/main.py" },
    	{ "src": "/product/(.*?)", "dest": "app/main.py" }
	]
}